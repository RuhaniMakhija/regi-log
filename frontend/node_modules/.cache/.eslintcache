lcome/Welcome.jsx",[],[],"/Users/ruhanifashionfantasy/login_register/frontendxerocodee/src/components/Login/Login.jsx",[],[],"/Users/ruhanifashionfantasy/login_register/frontendxerocodee/src/components/LoginRegister/LoginRegister.jsx",[],[],[],"/Users/ruhanifashionfantasy/login_register/frontendxerocodee/src/components/Register/Register.jsx",[],[],"/Users/ruhanifashionfantasy/login_register/frontendxerocodee/src/components/Developer/Developer.jsx",[],[],"/Users/ruhanifashionfantasy/login_register/frontendxerocodee/src/components/Selfhosting/Selfhosting.jsx",["95"],[],"import {React,useState,useEffect} from 'react'\nimport \"./selfhosting.css\"\nimport logo from \"../../images/logo.png\"\nimport SelfRepo from '../SelfRepo/SelfRepo';\n\n\nconst CLIENT_ID=\"41380882353b3c3a2910\";\nconst Selfhosting = () => {\n    const [accessToken, setAccessToken] = useState(null);\n    const [rerender,setRerender]=useState(false);\n    const [userData, setUserData]=useState([]);\n    const [repoNames, setRepoNames] = useState([]);\n    const [activeButton, setActiveButton] = useState(null);\n    const [code, setCodeParam]=useState();\n    const SelfClickhandler=(buttonname)=>{\n        setActiveButton(buttonname);\n      }\n\n      useEffect(() => {\n        const queryString = window.location.search;\n        const urlParams = new URLSearchParams(queryString);\n        const codeParam = urlParams.get(\"code\");\n        setCodeParam(codeParam)\n        \n        console.log(codeParam);\n    \n        if (codeParam && localStorage.getItem(\"accessToken\") === null) {\n            async function getAccessToken() {\n                try {\n                    const response = await fetch(\"http://localhost:5000/getAccessToken?code=\" + codeParam, {\n                        method: \"GET\"\n                    });\n    \n                    if (response.ok) {\n                        const data = await response.json();\n                        console.log(data);\n                       \n                        setUserData(data)\n                        console.log(data)\n                        if (data.access_token) {\n                            localStorage.setItem(\"accessToken\", data.access_token);\n                            setAccessToken(data.access_token);\n                            setRerender(!rerender);\n                        }\n                    } else {\n                        console.error(\"Error fetching access token\");\n                    }\n                } catch (error) {\n                    console.error(\"Error:\", error);\n                }\n            }\n            getAccessToken();\n        }\n    },[accessToken]);\n\n    async function getUserData(){\n        await fetch(\"http://localhost:5000/getUserData\",{\n            method: \"GET\",\n            headers: {\n                \"Authorization\" :\"Bearer \" +localStorage.getItem(\"accessToken\")\n            }\n        }).then((response)=>{\n            return response.json();\n        }).then((data)=>{\n            console.log(data);\n            setUserData(data)\n            return fetch(data.repos_url, {\n                method: \"GET\",\n                headers: {\n                    \"Authorization\": \"Bearer \" + localStorage.getItem(\"accessToken\")\n                }\n            });\n        })\n        .then(response => {\n            return response.json();\n        })\n        .then(repos => {\n            console.log(repos); // Just for debugging, remove this line\n\n            // Extract repository names\n            const names = repos.map(repo => repo.name);\n            setRepoNames(names); // Set the repository names in the state\n        })\n        .catch(error => {\n            console.error(\"Error:\", error);\n        });\n        \n    }\n\n\n\n      const loginWithGithub=()=>{\n        window.location.assign(\"https://github.com/login/oauth/authorize?client_id=\"+CLIENT_ID);\n      }\n  return (\n    <div>\n      <div className='self-img-container'>\n        <img src={logo}  className='self-logo' alt='logo'/>\n      </div>\n      \n\n      {\n        Object.keys(userData).length === 0  ? (\n            <>\n            <div className='self-div-info'>\n        <h1 className='self-heading'>Welcome Arya Soni !</h1>\n        <p className='self-para'>Choose from the following Deployment options</p>\n      </div>\n            <div className='self-btn-container'>\n                <button className={`self-btn ${activeButton===\"AWS Cloud\"? \"col-blue\":\"\"}`}\n                onClick={()=>SelfClickhandler('AWS Cloud')}>\n                    AWS Cloud\n                </button>\n\n                <button className={`self-btn ${activeButton===\"Github\"? \"col-blue\":\"\"}`}\n                onClick={loginWithGithub}>\n                    Github\n                </button>\n            </div>\n            </>\n        ):\n        (\n<>\n\n</>\n        )\n      }\n      {window.location.href === \"http://localhost:3001/self?code=\"+code && (\n        <div className='container-get-repo'>\n            <button onClick={getUserData} className='get-repo'>Repository List</button>\n            </div>\n          )}\n      \n      \n      \n      \n        {repoNames.length !== 0 ? (\n            <>\n            <div className='self-div-info'>\n            <h1 className='self-heading'> Welcome {userData.name}</h1>\n            <p className='self-para'>Github Repository List</p>\n            </div>\n            <SelfRepo repoNames={repoNames} />\n            </>\n) : (\n    <></>\n)}\n      {\n        activeButton===\"Github\" && (\n            <div className='organisation-input'>\n                    <input type=\"text\" className='input-or' placeholder=\"Github Repository\" />\n                    <button className='btn-or'>Submit</button>\n                </div>\n        )\n    }\n    </div>\n    \n  )\n}\n\nexport default Selfhosting\n","/Users/ruhanifashionfantasy/login_register/frontendxerocodee/src/components/SelfRepo/SelfRepo.jsx",["96"],[],"import React from 'react'\nimport logo from \"../../images/logo.png\"\n\nconst SelfRepo = ({repoNames}) => {\n    \n    if (!repoNames) {\n        return <p>Loading...</p>; // or any other loading indicator\n    }\n  return (\n    <div>\n      \n      \n      <div>\n       \n        {repoNames.map(repoName => (\n                    <li key={repoName}>{repoName}</li>\n                ))}\n        \n      </div>\n    </div>\n  )\n}\n\nexport default SelfRepo\n",{"ruleId":"97","severity":1,"message":"98","line":3,"column":27,"nodeType":"99","messageId":"100","endLine":3,"endColumn":33},{"ruleId":"97","severity":1,"message":"101","line":5,"column":8,"nodeType":"99","messageId":"100","endLine":5,"endColumn":21},{"ruleId":"97","severity":1,"message":"102","line":5,"column":30,"nodeType":"99","messageId":"100","endLine":5,"endColumn":41},{"ruleId":"97","severity":1,"message":"103","line":6,"column":8,"nodeType":"99","messageId":"100","endLine":6,"endColumn":18},{"ruleId":"97","severity":1,"message":"98","line":3,"column":27,"nodeType":"99","messageId":"100","endLine":3,"endColumn":33},{"ruleId":"97","severity":1,"message":"101","line":5,"column":8,"nodeType":"99","messageId":"100","endLine":5,"endColumn":21},{"ruleId":"104","severity":1,"message":"105","line":54,"column":7,"nodeType":"106","endLine":54,"endColumn":20,"suggestions":"107"},{"ruleId":"97","severity":1,"message":"108","line":2,"column":8,"nodeType":"99","messageId":"100","endLine":2,"endColumn":12},"no-unused-vars","'Router' is defined but never used.","Identifier","unusedVar","'LoginRegister' is defined but never used.","'GoogleLogin' is defined but never used.","'jwt_decode' is defined but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'rerender'. Either include it or remove the dependency array. You can also do a functional update 'setRerender(r => ...)' if you only need 'rerender' in the 'setRerender' call.","ArrayExpr